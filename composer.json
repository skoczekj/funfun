{
    "name": "root/www",
    "autoload": {
        "psr-4": {
            "Root\\Www\\": "bin/"
        }
    },
    "require": {
        "imangazaliev/didom": "^2.0",
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.9"
    },
    "scripts": {
        "lint": "phpcs --standard=PSR12 -n bin/",
        "lint-include-warnings": "phpcs --standard=PSR12 bin/",
        "lint-fix": "phpcbf --standard=PSR12 -n bin/"
    }
}
